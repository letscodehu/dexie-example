{
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "**/*.test.ts"
    ],
    "compilerOptions": {
        "outDir": "dist/",
        "target": "es2016",
        "module": "commonjs",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "declaration": true,
        "skipLibCheck": true
    }
}
